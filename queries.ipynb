   "source": [
    "### Customers with the most credit card transactions"
   ]
  },
  {
   "cell_type": "raw",
   "id": "2632e23f-d242-4864-a9a9-e4cc4a7fe18f",
   "metadata": {},
   "source": [
    "MATCH (c:Customer)-[:HAS]->(cc:Credit_card)-[:BUY]->(p:Purchase)\n",
    "RETURN \n",
    "    c.first_name + ' ' + c.last_name AS name,\n",
    "    COUNT(p) AS total_purchase,\n",
    "    SUM(p.amount) AS total_amount\n",
    "ORDER BY COUNT(p) DESC \n",
    "LIMIT 10"
   ]
  },
