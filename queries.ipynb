   "source": [
    "### Most valuable merchants"
   ]
  },
  {
   "cell_type": "raw",
   "id": "03200d2d-e05f-458e-a5e0-2d8467f4f634",
   "metadata": {},
   "source": [
    "MATCH (t:Purchase)<-[:SELL]-(m:Merchant)\n",
    "RETURN m.name, SUM(t.amount) AS total\n",
    "ORDER BY SUM(t.amount) DESC\n",
    "LIMIT 10"
   ]
  },
